{
  "extends": "tsconfig/nextjs.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "skipLibCheck": true,
    "noImplicitReturns": false,
    "paths": {
      "@/*": ["./*"],
    }
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "next-env.d.ts", "types.d.ts"],
  "exclude": ["**/node_modules", ".next/**"]
}
